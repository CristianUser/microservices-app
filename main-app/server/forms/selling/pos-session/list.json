{
  "type": "list",
  "routePath": "/pos-sessions",
  "title": "Pos Sessions",
  "props": {
    "apiRoutePrefix": "/selling/pos-session",
    "toNewDoc": {
      "type": "modal",
      "to": "/pos-session/{id}"
    },
    "title": "Pos Sessions",
    "breadcrumbRoutes": [
      {
        "path": "/",
        "breadcrumbName": "Home"
      },
      {
        "path": "/pos-sessions",
        "breadcrumbName": "Pos Sessions"
      }
    ],
    "columns": [
      {
        "title": "Name",
        "dataIndex": "employee",
        "render": {
          "type": "link",
          "to": "/pos-session/{id}"
        }
      },
      {
        "title": "Status",
        "dataIndex": "status"
      },
      {
        "title": "Date",
        "dataIndex": "startDate",
        "width": 180,
        "render": {
          "type": "date",
          "format": "yyyy/MM/dd HH:mm"
        }
      },
      {
        "title": "ID",
        "dataIndex": "cid",
        "width": 30
      }
    ]
  }
}
