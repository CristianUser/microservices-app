{
  "type": "list",
  "routePath": "/items",
  "title": "Items",
  "props": {
    "apiRoutePrefix": "/item/item",
    "toNewDoc": "/item/new",
    "title": "Items",
    "breadcrumbRoutes":[
      {
        "path": "/",
        "breadcrumbName": "Home"
      },
      {
        "path": "/items",
        "breadcrumbName": "Items"
      }
    ],
    "columns": [
      {
        "title": "Name",
        "dataIndex": "name",
        "searchable": true,
        "render": {
          "type": "link",
          "to": "/item/{id}"
        }
      },
      {
        "title": "Status",
        "dataIndex": "status",
        "filters": [
          { "text": "Draft", "value": "draft" },
          { "text": "Active", "value": "active" },
          { "text": "Archived", "value": "archived" }
        ]
      },
      {
        "title": "ID",
        "dataIndex": "cid",
        "sorter": true,
        "width": 30
      }
    ]
  }
}
